{
  "name": "no-code-secrets-manager",
  "description": "AI Secrets Orchestrator SaaS",
  "type": "build",
  "billing": {
    "plan": "free"
  },
  "vcsData": {
    "type": "upload",
    "upload": {
      "files": "northflank-deploy.zip"
    }
  },
  "buildSettings": {
    "dockerfile": "Dockerfile",
    "buildContext": "."
  },
  "deploy": {
    "replicas": 1,
    "resources": {
      "cpu": "0.25",
      "memory": "512Mi"
    },
    "healthCheck": {
      "path": "/",
      "port": 3000,
      "initialDelaySeconds": 30,
      "periodSeconds": 10,
      "timeoutSeconds": 5,
      "failureThreshold": 3
    }
  },
  "networking": {
    "ports": [
      {
        "port": 3000,
        "protocol": "http",
        "public": true
      }
    ]
  },
  "environment": {
    "NODE_ENV": "production",
  "VITE_APP_NAME": "VaultCraft",
    "VITE_APP_VERSION": "1.0.0",
    "VITE_APP_DESCRIPTION": "AI Secrets Orchestrator for no-coders"
  }
}
