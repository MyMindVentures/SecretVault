# VaultCraft Environment Variables
# Copy this file to .env.local and fill in your values

# App Configuration
VITE_APP_NAME=VaultCraft
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI Secrets Orchestrator for no-coders
VITE_ENVIRONMENT=production

# API Configuration
VITE_API_BASE_URL=https://api.nocodesecrets.com
VITE_WS_URL=wss://ws.nocodesecrets.com

# Authentication (Auth0)
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id
VITE_AUTH0_AUDIENCE=your-api-audience
VITE_AUTH0_REDIRECT_URI=https://nocodesecrets.com

# AI Services
VITE_OPENAI_API_KEY=your-openai-key
VITE_ANTHROPIC_API_KEY=your-anthropic-key

# Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-key

# Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=your-ga-id
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature Flags
VITE_ENABLE_AI_DISCOVERY=true
VITE_ENABLE_DOPPLER_INTEGRATION=true
VITE_ENABLE_PROVIDER_TEMPLATES=true
VITE_ENABLE_INTEGRATION_GRAPH=true
VITE_ENABLE_AUTO_PROVISIONING=true



